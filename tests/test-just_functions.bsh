begin_test "Need tty"
(
  setup_test

  function powershell()
  {
    echo "${result}"
  }

  unset SSH_TTY
  unset JUST_IS_TTY

  OS=not_windows not need_tty
  OS=Windows_NT JUST_IS_TTY=1 not need_tty
  JUST_IS_TTY=0 OS=Windows_NT need_tty

